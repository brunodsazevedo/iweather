{
  "expo": {
    "name": "iweather",
    "slug": "iweather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#13131A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brunodsazevedo.iweather"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#13131A"
      },
      "package": "com.brunodsazevedo.iweather"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/nunito/Nunito_400Regular.ttf",
            "node_modules/@expo-google-fonts/nunito/Nunito_700Bold.ttf",
            "node_modules/@expo-google-fonts/nunito/Nunito_800ExtraBold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0ecf92d6-5759-400e-9ddc-6e450659b7bf"
      }
    }
  }
}
